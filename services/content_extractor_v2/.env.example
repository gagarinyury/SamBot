# Database
DATABASE_URL=postgresql://sambot:password@postgres:5432/sambot_v2

# Audio storage
AUDIO_STORAGE_PATH=/app/audio_storage
AUTO_CLEANUP_DAYS=7

# yt-dlp settings
MAX_AUDIO_QUALITY=192
SOCKET_TIMEOUT=30
PREFERRED_AUDIO_FORMAT=mp3

# Chunking
DEFAULT_CHUNK_SIZE=500
DEFAULT_CHUNK_OVERLAP=50

# Service
SERVICE_PORT=8000
LOG_LEVEL=INFO